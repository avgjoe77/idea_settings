<application>
  <component name="CommunityFileTypes" version="11">
    <ignoreFiles list="CVS;SCCS;RCS;rcs;.DS_Store;.svn;.pyc;.pyo;*.pyc;*.pyo;.git;*.hprof;_svn;.hg;*.lib;*~;__pycache__;.bundle;*.rbc;" />
    <extensionMap>
      <mapping ext="jspx" type="XML" />
      <mapping ext="tagx" type="XML" />
    </extensionMap>
  </component>
  <component name="FileTypeManager" version="17">
    <extensionMap>
      <mapping ext="klib" type="ARCHIVE" />
      <mapping ext="cxx" type="C++" />
      <mapping pattern=".gitignore_global" type="Git file" />
      <mapping pattern="grains" type="PLAIN_TEXT" />
      <mapping pattern="Vagrantfile_template" type="PLAIN_TEXT" />
      <mapping pattern="foobar" type="PLAIN_TEXT" />
      <mapping pattern="Vagrantfile" type="PLAIN_TEXT" />
      <mapping ext="cfg" type="PLAIN_TEXT" />
      <mapping ext="ks" type="PLAIN_TEXT" />
      <mapping ext="crg" type="XML" />
      <mapping pattern="minion" type="YAML" />
      <mapping ext="sls" type="YAML" />
    </extensionMap>
  </component>
</application>